    <KnownFrameworkReference Update="Microsoft.NETCore.App"
                              DefaultRuntimeFrameworkVersion="5.0.0-dev"
                              LatestRuntimeFrameworkVersion="5.0.0-dev"
                              TargetingPackVersion="5.0.0-dev"
                              />
  </ItemGroup>-->

  <!--<ItemGroup>
    <PackageReference Include="Microsoft.NETCore.App.Runtime.win-x64" Version="5.0.0-dev" />
  </ItemGroup>-->

   <ItemGroup>
    <PackageReference Include="Microsoft.DotNet.ILCompiler" Version="6.0.0-*" />
  </ItemGroup>

   <ItemGroup>
     <ProjectReference Include="..\WinFormsComInterop\WinFormsComInterop.csproj" />
   </ItemGroup>

  <!--<ItemGroup Condition="'$(TargetFramework)' == 'netcoreapp5.0'">
    <ProjectReference Include="..\WinFormsComInterop\WinFormsComInterop.csproj" />
  </ItemGroup>-->

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <!--<Import Project="$(MSBuildSDKsPath)\Microsoft.NET.Sdk.WindowsDesktop\Sdk\Sdk.targets" />
